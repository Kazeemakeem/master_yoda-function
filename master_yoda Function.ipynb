    "def master_yoda(sentence):\n",
    "    split_sentence  = sentence.split()\n",
    "    reverse_elements = split_sentence[::-1]\n",
    "    reverse_sentence = \" \".join(reverse_elements)\n",
    "    return reverse_sentence"
